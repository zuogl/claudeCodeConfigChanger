{
  "name": "claude-config-changer",
  "version": "2.1.0",
  "description": "A CLI tool to switch between different Claude API configurations with instant shell activation",
  "main": "index.js",
  "bin": {
    "ccc": "bin/ccc.js",
    "ccs": "bin/ccs.js",
    "ccc-setup": "bin/ccc-setup.js"
  },
  "files": [
    "bin/",
    "lib/",
    "shell-wrapper.sh",
    "shell-wrapper.ps1",
    "postinstall.js",
    "install.sh",
    "install.ps1",
    "install.bat",
    "README.md"
  ],
  "scripts": {
    "start": "node bin/ccc.js",
    "test": "node -e \"console.log('No tests')\"",
    "postinstall": "node postinstall.js",
    "fix-line-endings": "perl -pi -e 's/\\r\\n/\\n/g; s/\\r/\\n/g' shell-wrapper.sh install.sh",
    "prepublishOnly": "npm run fix-line-endings && echo 'Fixed line endings and ready to publish to npm...'"
  },
  "keywords": [
    "claude",
    "config",
    "switch",
    "api",
    "anthropic",
    "hot-reload",
    "cli",
    "shell",
    "environment",
    "env"
  ],
  "author": "zuotongxue <zuogl448@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zuogl/claudeCodeConfigChanger.git"
  },
  "bugs": {
    "url": "https://github.com/zuogl/claudeCodeConfigChanger/issues"
  },
  "homepage": "https://github.com/zuogl/claudeCodeConfigChanger#readme",
  "dependencies": {
    "chalk": "^4.1.2",
    "inquirer": "^8.2.7"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
